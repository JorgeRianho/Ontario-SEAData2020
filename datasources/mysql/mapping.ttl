@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix : <http://example.org/affymetrix#> .

# --- Probeset_main ---
<#ProbesetMainMapping>
    a rr:TriplesMap ;
    rr:logicalTable [ rr:tableName "Probeset_main" ] ;
    rr:subjectMap [
        rr:template "http://example.org/affymetrix/probeset/{Probeset}" ;
        rr:class :Probeset ;
    ] ;
    rr:predicateObjectMap [ rr:predicate :inDataset ; rr:objectMap [ rr:column "inDataset" ] ] ;
    rr:predicateObjectMap [ rr:predicate :probeSetId ; rr:objectMap [ rr:column "probe_set_id" ] ] ;
    rr:predicateObjectMap [ rr:predicate :xRGD ; rr:objectMap [ rr:column "x_rgd" ] ] ;
    rr:predicateObjectMap [ rr:predicate :xMGI ; rr:objectMap [ rr:column "x_mgi" ] ] .

