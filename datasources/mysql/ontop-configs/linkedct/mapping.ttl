# --- agency_main ---
<#AgencyMainMapping>
    a rr:TriplesMap ;
    rr:logicalTable [ rr:tableName "agency_main" ] ;
    rr:subjectMap [
        rr:template "http://example.org/linkedct/agency/{agency}" ;
        rr:class :agency ;
    ] ;
    rr:predicateObjectMap [ rr:predicate :label ; rr:objectMap [ rr:column "label" ] ] ;
    rr:predicateObjectMap [ rr:predicate :agencyId ; rr:objectMap [ rr:column "agency_id" ] ] ;
