@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix : <http://example.org/kegg#> .

# --- Compound_main ---
<#CompoundMainMapping>
    a rr:TriplesMap ;
    rr:logicalTable [ rr:tableName "Compound_main" ] ;
    rr:subjectMap [
        rr:template "http://example.org/kegg/compound/{Compound}" ;
        rr:class :Compound ;
    ] ;
    rr:predicateObjectMap [ rr:predicate :label ; rr:objectMap [ rr:column "label" ] ] ;
    rr:predicateObjectMap [ rr:predicate :sameAs ; rr:objectMap [ rr:column "sameAs" ] ] ;
    rr:predicateObjectMap [ rr:predicate :identifier ; rr:objectMap [ rr:column "identifier" ] ] ;
    rr:predicateObjectMap [ rr:predicate :title ; rr:objectMap [ rr:column "title" ] ] ;
    rr:predicateObjectMap [ rr:predicate :formula ; rr:objectMap [ rr:column "formula" ] ] ;
    rr:predicateObjectMap [ rr:predicate :mass ; rr:objectMap [ rr:column "mass" ] ] ;
    rr:predicateObjectMap [ rr:predicate :url ; rr:objectMap [ rr:column "url" ] ] ;
    rr:predicateObjectMap [ rr:predicate :urlImage ; rr:objectMap [ rr:column "urlImage" ] ] .

