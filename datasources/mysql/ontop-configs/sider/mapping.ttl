@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix : <http://example.org/sider#> .

# --- drugs_main ---
<#DrugsMainMapping>
    a rr:TriplesMap ;
    rr:logicalTable [ rr:tableName "drugs_main" ] ;
    rr:subjectMap [
        rr:template "http://example.org/sider/drugs/{drugs}" ;
        rr:class :drugs ;
    ] ;
    rr:predicateObjectMap [ rr:predicate :label ; rr:objectMap [ rr:column "label" ] ] ;
    rr:predicateObjectMap [ rr:predicate :siderDrugId ; rr:objectMap [ rr:column "siderDrugId" ] ] ;
    rr:predicateObjectMap [ rr:predicate :stitchId ; rr:objectMap [ rr:column "stitchId" ] ] .

