        rr:class :side_effects ;
    ] ;
    rr:predicateObjectMap [ rr:predicate :label ; rr:objectMap [ rr:column "label" ] ] ;
    rr:predicateObjectMap [ rr:predicate :page ; rr:objectMap [ rr:column "page" ] ] ;
    rr:predicateObjectMap [ rr:predicate :sideEffectId ; rr:objectMap [ rr:column "sideEffectId" ] ] .

