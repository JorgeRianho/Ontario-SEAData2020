@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix : <http://example.org/diseasome#> .

# --- diseases_main ---
<#DiseasesMainMapping>
    a rr:TriplesMap ;
    rr:logicalTable [ rr:tableName "diseases_main" ] ;
    rr:subjectMap [
        rr:template "http://example.org/diseasome/diseases/{diseases}" ;
        rr:class :diseases ;
    ] ;
    rr:predicateObjectMap [ rr:predicate :label ; rr:objectMap [ rr:column "label" ] ] ;
    rr:predicateObjectMap [ rr:predicate :chromosomalLocation ; rr:objectMap [ rr:column "chromosomalLocation" ] ] ;
    rr:predicateObjectMap [ rr:predicate :class ; rr:objectMap [ rr:column "class" ] ] ;
    rr:predicateObjectMap [ rr:predicate :classDegree ; rr:objectMap [ rr:column "classDegree" ] ] ;
    rr:predicateObjectMap [ rr:predicate :degree ; rr:objectMap [ rr:column "degree" ] ] ;
    rr:predicateObjectMap [ rr:predicate :diseaseSubtypeOf ; rr:objectMap [ rr:column "diseaseSubtypeOf" ] ] ;
    rr:predicateObjectMap [ rr:predicate :name ; rr:objectMap [ rr:column "name" ] ] ;
    rr:predicateObjectMap [ rr:predicate :omim ; rr:objectMap [ rr:column "omim" ] ] ;
    rr:predicateObjectMap [ rr:predicate :omimPage ; rr:objectMap [ rr:column "omimPage" ] ] ;
    rr:predicateObjectMap [ rr:predicate :size ; rr:objectMap [ rr:column "size" ] ] .

