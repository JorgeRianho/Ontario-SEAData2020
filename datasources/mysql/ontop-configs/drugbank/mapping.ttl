# --- drug_interactions_main ---
<#DrugInteractionsMainMapping>
    a rr:TriplesMap ;
    rr:logicalTable [ rr:tableName "drug_interactions_main" ] ;
    rr:subjectMap [
        rr:template "http://example.org/drugbank/drug_interactions/{drug_interactions}" ;
        rr:class :drug_interactions ;
    ] ;
    rr:predicateObjectMap [ rr:predicate :label ; rr:objectMap [ rr:column "label" ] ] ;
