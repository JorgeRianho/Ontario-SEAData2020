@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix : <http://example.org/chebi#> .

# --- Compound_main ---
<#CompoundMainMapping>
    a rr:TriplesMap ;
    rr:logicalTable [ rr:tableName "Compound_main" ] ;
    rr:subjectMap [
        rr:template "http://example.org/chebi/compound/{Compound}" ;
        rr:class :Compound ;
    ] ;
    rr:predicateObjectMap [ rr:predicate :label ; rr:objectMap [ rr:column "label" ] ] ;
    rr:predicateObjectMap [ rr:predicate :identifier ; rr:objectMap [ rr:column "identifier" ] ] ;
    rr:predicateObjectMap [ rr:predicate :modified ; rr:objectMap [ rr:column "modified" ] ] ;
    rr:predicateObjectMap [ rr:predicate :title ; rr:objectMap [ rr:column "title" ] ] ;
    rr:predicateObjectMap [ rr:predicate :url ; rr:objectMap [ rr:column "url" ] ] ;
    rr:predicateObjectMap [ rr:predicate :Status ; rr:objectMap [ rr:column "Status" ] ] ;
    rr:predicateObjectMap [ rr:predicate :image ; rr:objectMap [ rr:column "image" ] ] ;
    rr:predicateObjectMap [ rr:predicate :xSource ; rr:objectMap [ rr:column "xSource" ] ] ;
    rr:predicateObjectMap [ rr:predicate :isEnantiomerOf ; rr:objectMap [ rr:column "is_enantiomer_of" ] ] ;
    rr:predicateObjectMap [ rr:predicate :inchi ; rr:objectMap [ rr:column "inchi" ] ] ;
    rr:predicateObjectMap [ rr:predicate :smiles ; rr:objectMap [ rr:column "smiles" ] ] ;
    rr:predicateObjectMap [ rr:predicate :seeAlso ; rr:objectMap [ rr:column "seeAlso" ] ] .

